# ================================================
# Adaptix Customer Environment Template
# ================================================
# Copy this file to .env and fill in your values

# ==================== REQUIRED ====================

# Database
DB_USER=adaptix
DB_PASSWORD=your_strong_database_password_here

# RabbitMQ
MQ_USER=adaptix
MQ_PASSWORD=your_strong_mq_password_here

# Security
SECRET_KEY=your_very_long_random_secret_key_min_50_chars

# ==================== OPTIONAL ====================

# Version (default: latest)
VERSION=latest

# Registry (default: adaptix)
REGISTRY=adaptix

# ==================== LICENSE ====================

# Your license key (provided by Adaptix)
LICENSE_KEY=XXXX-XXXX-XXXX-XXXX

# Company ID (provided by Adaptix)
COMPANY_ID=your_company_id
